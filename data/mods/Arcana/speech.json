  {
    "type": "speech",
    "speaker": "mon_archon",
    "sound": "\"Soon, I will claim what is rightfully mine.\"",
    "volume": 40
  },
  {
    "type": "speech",
    "speaker": "mon_archon",
    "sound": "\"This body may die, but I am eternal.\"",
    "volume": 40
  },
  {
